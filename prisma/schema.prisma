generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FplEntry {
  id        Int      @id @default(autoincrement())
  week      Int
  player    String
  points    Int
  games     Int
  teamId    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@unique([week, player])
}

model GgEntry {
  id        Int      @id @default(autoincrement())
  week      Int
  player    String
  games     Int
  wins      Int
  fiveK     Int
  points    Int
  createdAt DateTime @default(now())

  @@unique([week, player])
}

model GgEntry2024 {
  id     Int    @id @default(autoincrement())
  player String
  week   Int
  games  Int
  wins   Int
  fiveK  Int
  points Int

  @@map("ggEntry2024")
}

model BetsEntry {
  id        Int      @id @default(autoincrement())
  week      Int
  player    String
  games     Int
  wins      Int
  points    Int
  createdAt DateTime @default(now())

  @@unique([week, player])
}

model PokerEntry {
  id        Int      @id @default(autoincrement())
  week      Int
  bearo     String
  games     Int
  wins      Int
  points    Int
  createdAt DateTime @default(now())

  @@unique([week, bearo])
}

model SevenOkerEntry {
  id        Int      @id @default(autoincrement())
  week      Int
  bearo     String
  games     Int
  wins      Int
  points    Int
  gamepoints Int     @default(0)
  createdAt DateTime @default(now())

  @@unique([week, bearo])
}

model SevenOkerEntry2024 {
  id         Int      @id @default(autoincrement())
  week       Int
  bearo      String
  games      Int
  wins       Int
  points     Int
  gamepoints Int?
  createdAt  DateTime @default(now())

  @@map("sevenOkerEntry2024")
}

model FifaEntry {
  id            Int      @id @default(autoincrement())
  team          String   @unique
  logo          String
  games         Int
  wins          Int
  draws         Int
  losses        Int
  goalsScored   Int
  goalsConceded Int
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}

